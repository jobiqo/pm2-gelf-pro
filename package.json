{
  "name": "pm2-gelf-pro",
  "version": "1.0.1",
  "description": "PM2 logging to Graylog using gelf-pro",
  "main": "app.js",
  "dependencies": {
    "gelf-pro": "^1.3.6",
    "pm2": "^5.1.2",
    "pmx": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sethblack/pm2-gelf-pro.git"
  },
  "config": {
    "graylogPort": 12201,
    "graylogHost": "127.0.0.1",
    "gelfAdapterName": "udp"
  },
  "author": "Seth Black <sblack@sethserver.com>",
  "license": "Apache 2",
  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.0"
  }
}
